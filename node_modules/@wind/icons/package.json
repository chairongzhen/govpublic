{
  "name": "@wind/icons",
  "version": "1.0.4",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "unpkg": "./dist/index.umd.js",
  "sideEffects": false,
  "contributors": [
    "yfjiang<yfjiang@wind.com.cn>",
    "zahe<zahe@wind.com.cn>"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "./scripts/cleanup.bat",
    "start": "cross-env NODE_ENV=development father doc dev --storybook",
    "doc": "father doc build --storybook && node scripts/replaceFavicon.js",
    "doc-test": "http-server ./.doc",
    "lint": "eslint src/ --ext .tsx,.ts",
    "compile": "father build",
    "test": "father test",
    "ci": "NODE_ENV=ci npm run prepublishOnly",
    "test:local": "npm run generate && npm run compile && npm run test",
    "generate": "rimraf src/icons && cross-env TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/generate.ts --target=icon",
    "post-compile": "yarn run clean && cross-env TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/generate.ts --target=entry",
    "pub": "npm run generate && npm run compile",
    "svg": "node scripts/replaceModuleToCommonjs.js && cross-env NODE_ENV=production gulp --require ts-node/register/transpile-only",
    "build": "node scripts/replaceModuleToCommonjs.js && npm run svg && node scripts/replaceModuleToEsnext.js && npm run pub && node scripts/copy.js",
    "copy": "node scripts/copy.js"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@rgrove/parse-xml": "^2.0.2",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/gulp": "^4.0.6",
    "@types/gulp-concat": "^0.0.32",
    "@types/gulp-rename": "^0.0.33",
    "@types/insert-css": "^2.0.0",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.136",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.template": "^4.4.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/node": "^13.9.3",
    "@types/ramda": "^0.26.43",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/rgrove__parse-xml": "^1.1.0",
    "@types/svgo": "^1.3.1",
    "@types/through2": "^2.0.34",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@wind/wind-ui": "^2.1.0",
    "cross-env": "^5.2.0",
    "del": "^5.1.0",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^7.1.0",
    "father": "^2.16.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.0",
    "gulp": "4.0.2",
    "gulp-clone": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.9",
    "gulp-rename": "^2.0.0",
    "http-server": "^0.12.3",
    "jest": "^25.1.0",
    "lodash": "^4.17.19",
    "lodash.camelcase": "^4.3.0",
    "lodash.template": "^4.5.0",
    "lodash.upperfirst": "^4.3.1",
    "mobx": "^5.1.0",
    "mobx-react": "^6.1.3",
    "ramda": "^0.27.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "rimraf": "^3.0.0",
    "styled-components": "^3.3.3",
    "svgo": "^1.3.2",
    "through2": "^3.0.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.0.3",
    "vinyl": "^2.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "classnames": "^2.2.6",
    "insert-css": "^2.0.0",
    "rc-util": "^5.0.1"
  }
}
